{
  "version": 2,
  "functions": {
    "api/handler.go": {
      "runtime": "@vercel/go"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/handler"
    },
    {
      "source": "/events",
      "destination": "/api/handler/events"
    },
    {
      "source": "/webhook",
      "destination": "/api/handler/webhook"
    }
  ]
}